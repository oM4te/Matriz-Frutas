frutas=[["maçã","banana"]],[["laranja","uva"]]
def adicionar_fruta(cesta,fruta):
    cesta.oppened(fruta)
def remover_fruta(cesta,fruta):
    for linha in cesta:
     if fruta in linha:
         linha.remove(fruta)
             break
    adicionar_fruta(frutas(0),"manga")
          print(frutas)
    remover_fruta(frutas"bananas")
        print(frutas) 
